              SQLTask:SqlStatementSource="INSERT INTO BISINTDATA.dbo.xinpWaehrgkurse(dtedit,luserkz,ldelete,xwkWaehrung,xwkBeginn,xwkKurs,xwkVonwaehrung,xwkZuwaehrung)&#xA;SELECT DISTINCT&#xA;         dtedit         = getdate()&#xA;       , luserkz        = convert(varchar(45) , 'Interface'            ) collate Latin1_General_CI_AS&#xA;       , ldelete        = convert(bit         , 0                      )&#xA;       , xwkWaehrung    = convert(varchar(256), 'EUR'                  ) collate Latin1_General_CI_AS&#xA;       , xwkBeginn      = xDate&#xA;       , xwkKurs        = rate&#xA;       , xwkVonwaehrung = convert(varchar(256), 'EUR'                  ) collate Latin1_General_CI_AS&#xA;       , xwkZuwaehrung  = convert(varchar(256),currency                ) collate Latin1_General_CI_AS&#xA;FROM dbo.EZB_Kurse&#xA;WHERE xDate &gt;= CASE WHEN 30=-1 THEN CONVERT(datetime,'1999.01.01',102) ELSE DATEADD(mm,- 30,getdate()) END UNION&#xA;SELECT   DISTINCT&#xA;         dtedit         = getdate()&#xA;       , luserkz        = convert(varchar(45) , 'Interface'            ) collate Latin1_General_CI_AS&#xA;       , ldelete        = convert(bit         , 0                      )&#xA;       , xwkWaehrung    = convert(varchar(256), currency               ) collate Latin1_General_CI_AS&#xA;       , xwkBeginn      = xDate&#xA;       , xwkKurs        = ROUND(1.E0/nullif(rate,0.E0),8)&#xA;       , xwkVonwaehrung = convert(varchar(256), currency               ) collate Latin1_General_CI_AS&#xA;       , xwkZuwaehrung  = convert(varchar(256), 'EUR'                  ) collate Latin1_General_CI_AS&#xA;FROM dbo.EZB_Kurse&#xA;WHERE xDate &gt;= CASE WHEN 30=-1 THEN CONVERT(datetime,'1999.01.01',102) ELSE DATEADD(mm,- 30,getdate()) END&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
