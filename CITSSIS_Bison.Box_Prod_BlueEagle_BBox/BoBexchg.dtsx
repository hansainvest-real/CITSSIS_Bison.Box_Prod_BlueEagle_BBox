                  SQLTask:SqlStatementSource="EXEC('&#xA;SET NOCOUNT ON&#xA;DECLARE @l varchar(max), @e varchar(256), @p int&#xA;SELECT @l=isnull(@l,'''')+name+'',''FROM syscolumns WHERE id=object_id(''dbo.dstatisVPILangeReihen'',''U'')AND NAME NOT IN(''Periode'',''lid'')&#xA;WHILE @l!=''''&#xA;BEGIN&#xA;  SET @p=charindex('','',@l)&#xA;  SET @e=left(@l,@p-1)&#xA;  SET @l=substring(@l,@p+1,2000000000)&#xA;  EXEC(''update dbo.dstatisVPILangeReihen SET [''+@e+'']=NULL WHERE LTRIM(RTRIM([''+@e+'']))=''''-'''''')&#xA;END;&#xA;&#xA;set @l=null;&#xA;SELECT @l=isnull(@l,'''')+name+'',''FROM syscolumns WHERE id=object_id(''dbo.dstatisVPILangeReihenJD'',''U'')AND NAME NOT IN(''lid'')&#xA;WHILE @l!=''''&#xA;BEGIN&#xA;  SET @p=charindex('','',@l)&#xA;  SET @e=left(@l,@p-1)&#xA;  SET @l=substring(@l,@p+1,2000000000)&#xA;  EXEC(''update dbo.dstatisVPILangeReihenJD SET [''+@e+'']=NULL WHERE LTRIM(RTRIM([''+@e+'']))=''''-'''''')&#xA;END;&#xA;&#xA;--   Jahr in jede Spalte ergänzen&#xA;WITH[X]AS(SELECT *,r=ROW_NUMBER()OVER(ORDER BY lid)FROM[dbo].[dstatisVPILangeReihen]WHERE BasisJahr=''2010'') UPDATE[Y]SET Jahr=(SELECT MAX(Jahr)FROM[X]WHERE r&lt;=Y.r)FROM[X][Y] WHERE Jahr IS NULL;&#xA;WITH[X]AS(SELECT *,r=ROW_NUMBER()OVER(ORDER BY lid)FROM[dbo].[dstatisVPILangeReihen]WHERE BasisJahr=''2015'') UPDATE[Y]SET Jahr=(SELECT MAX(Jahr)FROM[X]WHERE r&lt;=Y.r)FROM[X][Y] WHERE Jahr IS NULL;&#xA;&#xA;--  ungültige Sätze löschen&#xA;DELETE FROM [BISImportDB].[dbo].[dstatisVPILangeReihen] WHERE Monat is NULL&#xA;SET NOCOUNT OFF&#xA;')" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
