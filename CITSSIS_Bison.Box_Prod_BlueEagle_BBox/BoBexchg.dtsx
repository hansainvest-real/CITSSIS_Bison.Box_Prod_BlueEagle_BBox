                     SenderEMail VARCHAR(256),FREQ VARCHAR(256),BBK_ID VARCHAR(256),DECIMALS INT,UNIT VARCHAR(256),UNIT_MULT VARCHAR(256),
                     BBK_TITLE varchar(256),BBK_TITLE_ENG varchar(256),TIME_PERIOD varchar(12),OBS_VALUE float,BBK_DIFF float,BBK_DIFF_Y float,
                     BBK_COMM_OBS varchar(512),BBK_COMM_OBS_ENG varchar(512))

DECLARE @MyFiles varchar(max), @MyFile varchar(256), @p int, @ID varchar(256)
DECLARE @REF nvarchar(4000)

SELECT @MyFiles=F FROM OPENROWSET(BULK ''&amp;bis.IntPath\\&amp;Interface\\Program\\Geldmarkt.lst'',SINGLE_BLOB)x(F)
WHILE @MyFiles!=''''
BEGIN
  SET @p=CHARINDEX(convert(char(2),0x0d0a),@MyFiles+convert(char(2),0x0d0a))
  SET @MyFile=LEFT(@MyFiles,@p-1)
  SET @MyFiles=substring(@MyFiles,@p+2,20000000)
  SET @p=charindex(''/'',reverse(@MyFile)+''/'')-2
  SET @MyFile=replace(substring(@MyFile,len(@MyFile)-@p,256),''?'',''@'')
  SET @ID=LEFT(@MyFile,charindex(''&amp;'',@MyFile+''?'')-1)
  SET @ID=SUBSTRING(@ID,charindex(''='',@ID)+1,256)
  IF RTRIM(@MyFile)!=''''
  BEGIN
    INSERT INTO @TABLE
    EXECUTE(''
      DECLARE @xml varchar(max), @ptr int
      SELECT @xml=F FROM OPENROWSET(BULK ''''&amp;BIS.IntDataPath\\Geldmarkt\\''+@MyFile+'''''',SINGLE_BLOB)x(F)
      EXEC sp_xml_preparedocument
           @ptr out
