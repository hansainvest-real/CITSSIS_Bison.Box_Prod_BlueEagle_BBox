      SELECT *
      FROM OPENXML (@ptr,''''dft:CompactData/bbk:DataSet/bbk:Series/bbk:Obs'''',2)
      WITH (PREPAIRED        datetime     ''''../../../dft:Header/dft:Prepared'''',
            TEST             bit          ''''../../../dft:Header/dft:Test'''',
            Truncated        bit          ''''../../../dft:Header/dft:Truncated'''',
            SenderID         VARCHAR(256) ''''../../../dft:Header/dft:Sender/@id'''',
            Sender_lang      VARCHAR(256) ''''../../../dft:Header/dft:Sender/dft:Name/@xml:lang'''',
            SenderName       VARCHAR(256) ''''../../../dft:Header/dft:Sender/dft:Name'''',
            SenderEMail      VARCHAR(256) ''''../../../dft:Header/dft:Sender/dft:Contact/dft:Email'''',
            FREQ             VARCHAR(256) ''''../@FREQ'''',
            BBK_ID           VARCHAR(256) ''''../@BBK_ID'''',
            DECIMALS         VARCHAR(256) ''''../@DECIMALS'''',
            UNIT             VARCHAR(256) ''''../@UNIT'''',
            UNIT_MULT        VARCHAR(256) ''''../@UNIT_MULT'''',
            BBK_TITLE        varchar(256) ''''../@BBK_TITLE'''',
            BBK_TITLE_ENG    varchar(256) ''''../@BBK_TITLE_ENG'''',
            TIME_PERIOD      varchar(12)  ''''@TIME_PERIOD'''',
            OBS_VALUE        float        ''''@OBS_VALUE'''',
            BBK_DIFF         float        ''''@BBK_DIFF'''',
            BBK_DIFF_Y       float        ''''@BBK_DIFF_Y'''',
            BBK_COMM_OBS     varchar(512) ''''@BBK_COMM_OBS'''',
            BBK_COMM_OBS_ENG varchar(512) ''''@BBK_COMM_OBS_ENG'''')
     EXEC sp_xml_removedocument @ptr'')
  END
END

