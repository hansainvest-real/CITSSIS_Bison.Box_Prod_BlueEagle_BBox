          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">REPLACE(
"INSERT INTO BISINTDATA.dbo.xinpWaehrgkurse(dtedit,luserkz,ldelete,xwkWaehrung,xwkBeginn,xwkKurs,xwkVonwaehrung,xwkZuwaehrung)
SELECT DISTINCT
         dtedit         = getdate()
       , luserkz        = convert(varchar(45) , 'Interface'            ) collate Latin1_General_CI_AS
       , ldelete        = convert(bit         , 0                      )
       , xwkWaehrung    = convert(varchar(256), 'EUR'                  ) collate Latin1_General_CI_AS
       , xwkBeginn      = xDate
       , xwkKurs        = rate
       , xwkVonwaehrung = convert(varchar(256), 'EUR'                  ) collate Latin1_General_CI_AS
       , xwkZuwaehrung  = convert(varchar(256),currency                ) collate Latin1_General_CI_AS
FROM dbo.EZB_Kurse
WHERE xDate &gt;= CASE WHEN &amp;MM=-1 THEN CONVERT(datetime,'1999.01.01',102) ELSE DATEADD(mm,- &amp;MM,getdate()) END UNION
SELECT   DISTINCT
         dtedit         = getdate()
       , luserkz        = convert(varchar(45) , 'Interface'            ) collate Latin1_General_CI_AS
       , ldelete        = convert(bit         , 0                      )
       , xwkWaehrung    = convert(varchar(256), currency               ) collate Latin1_General_CI_AS
       , xwkBeginn      = xDate
       , xwkKurs        = ROUND(1.E0/nullif(rate,0.E0),8)
       , xwkVonwaehrung = convert(varchar(256), currency               ) collate Latin1_General_CI_AS
       , xwkZuwaehrung  = convert(varchar(256), 'EUR'                  ) collate Latin1_General_CI_AS
FROM dbo.EZB_Kurse
WHERE xDate &gt;= CASE WHEN &amp;MM=-1 THEN CONVERT(datetime,'1999.01.01',102) ELSE DATEADD(mm,- &amp;MM,getdate()) END
","&amp;MM",@varMM)</DTS:PropertyExpression>
