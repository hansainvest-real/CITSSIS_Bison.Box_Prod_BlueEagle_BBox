          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">REPLACE(REPLACE(
"INSERT INTO BISINTDATA.dbo.xinpIndexStamm(dtedit,luserKz,ldelete,xIndex,xixBezeichnung,xixBasisjahr,xIndexArt,xindexbezeichnung1,xExactDays,Version,Source,Strategy,Szenario,PlanProzess)
SELECT distinct
       dtedit             = getdate(),
       luserKz            = cast('Interface' as varchar( 45)) Collate Latin1_General_CI_AS ,
       ldelete            = cast(0 as bit),
       xIndex             = cast(isnull(xindexnr          ,BBK_ID) as varchar(50)) Collate Latin1_General_CI_AS,
       xixBezeichnung     = cast(isnull(xindexbezeichnung ,BBK_TITLE)  as varchar(256)) Collate Latin1_General_CI_AS,
       xixBasisjahr       = cast(isnull(xindexbasisjahr   ,(SELECT left(min(TIME_PERIOD),4)FROM dbo.BBK_Geldmarkt WHERE BBK_ID=x.BBK_ID)) as int),
--       xIndexArt          = cast(case when xindexart is null then 'Geldmarktsätze' end as varchar(256)) Collate Latin1_General_CI_AS,
       xIndexArt          = cast('02;Zinsindex°0' as varchar(256)) Collate Latin1_General_CI_AS,
       xindexbezeichnung1 = cast(isnull(xindexbezeichnung1,BBK_TITLE_ENG) as varchar(256)) Collate Latin1_General_CI_AS,
       xExactDays         = CONVERT(BIT,CASE WHEN BBK_ID LIKE 'BBK01.ST____'THEN'TRUE'ELSE'FALSE'END),
       Version            = (SELECT xkeyalphanr from(SELECT lid,xkeyalphanr FROM &amp;TGTDB.dbo.xlbikeyval UNION SELECT-1,'Alle')x where lid=indxStm_xVersion    ),
       Source             = (SELECT xkeyalphanr from(SELECT lid,xkeyalphanr FROM &amp;TGTDB.dbo.xlbikeyval UNION SELECT-1,'Alle')x where lid=indxStm_xSource     ),
       Strategy           = (SELECT xkeyalphanr from(SELECT lid,xkeyalphanr FROM &amp;TGTDB.dbo.xlbikeyval UNION SELECT-1,'Alle')x where lid=indxStm_xStrategy   ),
       Szenario           = (SELECT xkeyalphanr from(SELECT lid,xkeyalphanr FROM &amp;TGTDB.dbo.xlbikeyval UNION SELECT-1,'Alle')x where lid=indxStm_xSzenario   ),
       PlanProzess        = (SELECT xkeyalphanr from(SELECT lid,xkeyalphanr FROM &amp;TGTDB.dbo.xlbikeyval UNION SELECT-1,'Alle')x where lid=indxStm_xPlanProzess)
FROM dbo.BBK_Geldmarkt x
   ,(SELECT xindexnr,xindexbezeichnung,xindexbasisjahr,xindexart,xindexbezeichnung1,xixReferenz,IndxStm_xVersion,IndxStm_xSource,IndxStm_xStrategy,IndxStm_xSzenario,IndxStm_xPlanProzess
     FROM &amp;TGTDB.dbo.xlbiidxstamm WHERE'&amp;USEREF'='1' UNION
     SELECT null,null,null,null,null,null,null,null,null,null,null )ix
WHERE BBK_ID LIKE 'BBK01.S[TU]____'
  AND(xixReferenz=BBK_ID OR '&amp;USEREF'!='1')
order by 4
","&amp;TGTDB", @[User::cfgIDB]),"&amp;USEREF", @[User::varUseRef] )</DTS:PropertyExpression>
