          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">"INSERT INTO BISINTDATA.dbo.xinpIndexValue(dtedit,luserkz,ldelete,xivIndex,xivBasisjahr,xivJahr,xivMonat,xivWert,xivDatum)
SELECT DISTINCT
       dtedit       = getdate(),
       luserKz      = cast('Interface' as varchar( 45)) Collate Latin1_General_CI_AS ,
       ldelete      = cast(0 as bit),
       xivIndex     = cast(xIndexName as varchar(50)) Collate Latin1_General_CI_AS,
