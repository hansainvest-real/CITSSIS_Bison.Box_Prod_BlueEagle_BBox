              <DTS:ObjectData>
                <SQLTask:SqlTaskData
                  SQLTask:Connection="{44D116F0-C52E-41F4-B869-4F8A33413793}"
                  SQLTask:SqlStatementSource="WITH XMLNAMESPACES ('http://www.SDMX.org/resources/SDMXML/schemas/v2_0/message' AS message &#xA;      , 'http://www.SDMX.org/resources/SDMXML/schemas/v2_0/common' AS common &#xA;      , 'http://www.w3.org/2001/XMLSchema-instance'     AS xsi    &#xA;      , 'http://www.SDMX.org/resources/SDMXML/schemas/v2_0/compact' AS compact&#xA;      , 'http://www.SDMX.org/resources/SDMXML/schemas/v2_0/message' AS T      &#xA;      , 'http://oecd.stat.org/Data'         AS OECD  &#xA;       )&#xA;INSERT INTO dbo.xinpIndexstamm (dtedit, luserKz, ldelete, xIndex, xixBezeichnung, xixBasisjahr, xIndexArt, IsMarketIndex, xindexbezeichnung1, xindexFunction, xindexIsFunction, xixReferenz, xExactDays, xdtaSrcHub, xdtaSrcPFX, xdtaSrcDlvryTyp, xindexCountry, xindexgroup, xindexGroupBaseYear, xixBezeichnung_intf)&#xA;SELECT [dtedit]              = GETDATE()&#xA;     , [luserKz]             = CONVERT(VARCHAR(45) , 'Interface') &#xA;     , [ldelete]             = CONVERT(bit         , 'false')&#xA;     , [xIndex]              = CONVERT(VARCHAR(256), x0.pfx+'VPI'+CASE WHEN xml.loc!='DEU'THEN''ELSE''END + x1.BaseYear )&#xA;     , [xixBezeichnung]      = CONVERT(VARCHAR(256), 'Verbraucherpreisindex, Basis '+ x1.BaseYear + ' ('+xml.loc + ')')&#xA;     , [xixBasisjahr]   = CONVERT(INT         , x1.BaseYear   )&#xA;     , [xIndexArt]    = CONVERT(VARCHAR(256), '04'          )             ---  Sonstiges&#xA;     , [IsMarketIndex]   = CONVERT(BIT         , 'false'       )&#xA;     , [xindexbezeichnung1]  = CONVERT(VARCHAR(256), NULL          )&#xA;     , [xindexFunction]   = CONVERT(VARCHAR(256), NULL          )&#xA;     , [xindexIsFunction]  = CONVERT(VARCHAR(256), NULL          )&#xA;     , [xixReferenz]   = CONVERT(VARCHAR(256), NULL          )&#xA;     , [xExactDays]    = CONVERT(BIT         , 'false'       )&#xA;     , [xdtaSrcHub]    = CONVERT(VARCHAR(256), 'www.oecd.org')&#xA;     , [xdtaSrcPFX]    = CONVERT(VARCHAR(256), 'OECD_'       )&#xA;     , [xdtaSrcDlvryTyp]  = CONVERT(VARCHAR(256), 'XML'         )&#xA;     , [xindexCountry]   = CONVERT(VARCHAR(256), [xml].loc     )&#xA;     , [xindexgroup]   = CONVERT(VARCHAR(256), NULL          )&#xA;     , [xindexGroupBaseYear] = CONVERT(VARCHAR(256), NULL          )&#xA;     , [xixBezeichnung_intf] = CONVERT(VARCHAR(256), '2'           )&#xA;FROM(SELECT N0.node.value('@LOCATION[1]'       ,'varchar(256)') AS loc &#xA;          , N0.node.value('@SUBJECT[1]'        ,'varchar(256)') AS subj  &#xA;          , N0.node.value('@MEASURE[1]'        ,'varchar(256)') AS meas  &#xA;          , N0.node.value('@FREQUENCY[1]'      ,'varchar(256)') AS freq  &#xA;          , N0.node.value('@TIME_FORMAT[1]'    ,'varchar(256)') AS timeformat &#xA;          , N0.node.value('@UNIT[1]'           ,'varchar(256)') AS unit  &#xA;          , N0.node.value('@POWERCODE[1]'      ,'varchar(256)') AS powercode &#xA;          , N0.node.value('@REFERENCEPERIOD[1]','varchar(256)') AS refperiod &#xA;     FROM(SELECT CONVERT(xml, ?) AS F)x&#xA;     CROSS APPLY F  .nodes('message:CompactData/OECD:DataSet/OECD:Series')N0(node)&#xA;    )AS[xml] &#xA;JOIN(VALUES('AUT','AT_'),('DEU',''),('NLD','NL_'),('CZE','CZ_'),('FRA','FR'),('ITA','IT_'),('ESP','ES_'),('DNK','DK_'),('SWE','SW_'),('FIN','FI_'),('GBR','GB'),('USA','US_'),('CHE','CH_')&#xA;    )x0(loc,pfx) ON [xml].loc = x0.loc&#xA;CROSS APPLY(SELECT LEFT(refperiod,NULLIF(CHARINDEX('_',refperiod),0)-1))x1(BaseYear)&#xA;WHERE xml.subj != 'CPALTT01'&#xA;  AND xml.loc in ('AUT','DEU')" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
