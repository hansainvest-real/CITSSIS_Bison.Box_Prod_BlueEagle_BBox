              <DTS:PropertyExpression
                DTS:Name="SqlStatementSource">"EXEC('
SET NOCOUNT ON
DECLARE @l varchar(max), @e varchar(256), @p int
SELECT @l=isnull(@l,'''')+name+'',''FROM syscolumns WHERE id=object_id(''dbo.dstatisVPILangeReihen'',''U'')AND NAME NOT IN(''Periode'',''lid'')
WHILE @l!=''''
BEGIN
  SET @p=charindex('','',@l)
  SET @e=left(@l,@p-1)
  SET @l=substring(@l,@p+1,2000000000)
  EXEC(''update dbo.dstatisVPILangeReihen SET [''+@e+'']=NULL WHERE LTRIM(RTRIM([''+@e+'']))=''''-'''''')
END;

set @l=null;
SELECT @l=isnull(@l,'''')+name+'',''FROM syscolumns WHERE id=object_id(''dbo.dstatisVPILangeReihenJD'',''U'')AND NAME NOT IN(''lid'')
WHILE @l!=''''
BEGIN
  SET @p=charindex('','',@l)
  SET @e=left(@l,@p-1)
  SET @l=substring(@l,@p+1,2000000000)
  EXEC(''update dbo.dstatisVPILangeReihenJD SET [''+@e+'']=NULL WHERE LTRIM(RTRIM([''+@e+'']))=''''-'''''')
END;

--   Jahr in jede Spalte ergänzen
WITH[X]AS(SELECT *,r=ROW_NUMBER()OVER(ORDER BY lid)FROM[dbo].[dstatisVPILangeReihen]WHERE BasisJahr=''2010'') UPDATE[Y]SET Jahr=(SELECT MAX(Jahr)FROM[X]WHERE r&lt;=Y.r)FROM[X][Y] WHERE Jahr IS NULL;
WITH[X]AS(SELECT *,r=ROW_NUMBER()OVER(ORDER BY lid)FROM[dbo].[dstatisVPILangeReihen]WHERE BasisJahr=''2015'') UPDATE[Y]SET Jahr=(SELECT MAX(Jahr)FROM[X]WHERE r&lt;=Y.r)FROM[X][Y] WHERE Jahr IS NULL;

--  ungültige Sätze löschen
DELETE FROM [BISImportDB].[dbo].[dstatisVPILangeReihen] WHERE Monat is NULL
SET NOCOUNT OFF
')"</DTS:PropertyExpression>
