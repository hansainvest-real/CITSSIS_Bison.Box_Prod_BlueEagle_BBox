            <SQLTask:SqlTaskData
              SQLTask:Connection="{776C6DFC-903B-457E-8010-EDEF3CA338EB}"
              SQLTask:SqlStatementSource="&#x9;    WITH &#xA;        cte_xFakt AS &#xA;        (&#xA; SELECT&#xA; r = ROW_NUMBER() OVER (&#xA; PARTITION BY &#xA; CASE WHEN attr.xAttrAlphanr IN ('010', '030') THEN 'IST' ELSE 'ISTW' END&#xA; , CASE WHEN attr.xAttrAlphanr IN ('010', '020') THEN 'ST460' WHEN attr.xAttrAlphanr IN ('030', '031') THEN 'ST465' END&#xA; , obj.xobjnr&#xA; ORDER BY CONVERT(CHAR(6), xAttrValidFrom, 112)&#xA; )&#xA; , fDim01        = CASE WHEN attr.xAttrAlphanr IN ('010', '020') THEN 'ST460' WHEN attr.xAttrAlphanr IN ('030', '031') THEN 'ST465' END&#xA; , fDim02        = obj.xobjnr&#xA; , fDim03        = (SELECT xgesellschaftcode FROM xgesellschaft WHERE xgesellschaftid = obj.xobjgesellsch)&#xA; , fPeriod        = CONVERT(CHAR(6), xAttrValidFrom, 112)&#xA; , fVersion        = CASE WHEN attr.xAttrAlphanr IN ('010', '030') THEN 'IST' ELSE 'ISTW' END&#xA; , fValue        = attrInh.xAttrVar&#xA; , fCurrency        = CASE WHEN attr.xAttrAlphanr IN ('010', '030') THEN 'EUR' ELSE ISNULL((select xKeyAlphaNr from dbo.xlbiKeyVal where lid = xobjwaehrung), 'EUR') END&#xA; -- SELECT attrInh.*&#xA; FROM dbo.xlbiAttribute attr&#xA; JOIN dbo.xlbiAttributeInh attrInh ON attr.lparentid = attrInh.xAttrGrpID and attr.lid = attrInh.xAttrID&#xA; JOIN dbo.xlbiobjekte obj ON attrInh.lparentid = obj.lid&#xA; WHERE attrInh.xAttrGrpID = (SELECT lid FROM dbo.xlbiAttributeGrp WHERE xAttrGrpAlphanr = 'OBJ_STAMM_ANKAUF' AND xMenuID = 3)&#xA; AND attrInh.MENUID = 3 -- 3 = Objekt&#xA; AND obj.ldelete = 0 -- 20210412_JaVo ldelete eingefügt (671821587)&#xA; /*&#xA; 010 Nettokaufpreis (NKP) inkl. nachträglich aktivierte Kosten (in €)&#xA; 020 Nettokaufpreis (NKP) inkl. nachträglich aktivierte Kosten (in Währung)&#xA; 030 Erwerbsnebenkosten (in €)&#xA; 031 Erwerbsnebenkosten (in Währung)&#xA; */&#xA; AND attr.xAttrAlphanr IN ('010', '030', '020', '031')&#xA; AND xAttrValidFrom IS NOT NULL&#xA; AND obj.xobjgesellsch IS NOT NULL&#xA;        )&#xA;&#xA;        ,        cte_xFakt_withNxtEnds        AS (&#xA; SELECT cur.*, fPeriodEnd = ISNULL(CONVERT(CHAR(6), DATEADD(MONTH, -1, nxt.fPeriod+'01'), 112), CONVERT(CHAR(6), GETDATE(), 112)) &#xA; FROM cte_xFakt cur &#xA; LEFT JOIN cte_xFakt nxt ON cur.fVersion = nxt.fVersion AND cur.fDim01 = nxt.fDim01 AND cur.fDim02 = nxt.fDim02 AND cur.r + 1 = nxt.r&#xA; )&#xA;&#xA;        INSERT INTO BISDBOBDB.dbo.xfakt (fDim01, fDim02, fDim03, fDim04, fDim05, fDim06, fDim07, fPart, fPeriod, fVersion, fValue, fCurrency, fUser, fTime)&#xA;        SELECT fDim01, fDim02, fDim03, fDim04 = 'NIX', fDim05 = 'NIX', fDim06 = 'NIX', fDim07 = 'NIX', fPart = 'STATM', fPeriod=p.dperiodid, fVersion, fValue, fCurrency, fUser = 'INTERFACE', fTime = GETDATE()&#xA;        FROM cte_xFakt_withNxtEnds f&#xA;        JOIN BISDBOBDB.dbo.tPeriod p ON p.dperiodid BETWEEN f.fperiod AND f.fPeriodEnd&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
          </DTS:ObjectData>
        </DTS:Executable>
