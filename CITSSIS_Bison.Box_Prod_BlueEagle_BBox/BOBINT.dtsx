                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT FVERSION='IST',FPERIOD='190001',FPART,FDIM01,FDIM02,FDIM03,FDIM04,FDIM05='NIX',FDIM06='NIX',FDIM07='NIX',FVALUE=SUM(FVALUE),FUSER='INTERFACE',FTIME=GETDATE()
FROM
(
    select
      FDIM01	= ktn.Konto
    , FDIM02	= xvertrnr
    , FDIM03	= ge.xgesellschaftcode
	, FDIM04	= isnull(dbo.BISGET_Keyval_Alpha(xkautart), 'NIX')
    , FPART		= PRT.PART
    , FVALUE	= case when ktn.Konto = 'K100' then kaut.xkautsoll*(-1) else kaut.xkauthaben end
    -- select *
    from xlbikaution kaut
    join xlbivertrag vtg on kaut.lparentid = vtg.lid
    join xlbiflaeche fl on vtg.xflaechennr = fl.lid
    join xlbigrundgeb gg on fl.lparentid = gg.lid
    join xlbiobjekte obj on gg.lparentid = obj.lid
    join xgesellschaft ge on obj.xobjgesellsch = ge.xgesellschaftid
    cross join (select 'K100' as Konto union select 'K200') ktn
    cross join (select 'MIBU' as PART) PRT --RW zunächst deaktiviert, da scheinbar über STAT-Standard union select 'STAT') PRT
    where kaut.ldelete = 0
    and vtg.ldelete = 0
) ktn
GROUP BY FDIM01, FDIM02, FDIM03, FDIM04, FPART</property>
