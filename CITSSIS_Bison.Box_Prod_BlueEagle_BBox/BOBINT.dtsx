              <DTS:VariableValue
                DTS:DataType="8">declare @StartP as varchar(6)
declare @PlanP as varchar(6)
declare @MaxP as varchar(6)
declare @VarGrpID int 

IF object_id('BISIBOBDB.dbo.G_GesbtlgPath_Period') is not null BEGIN 

select @VarGrpID = GZ00IDNR from dbo.TVARGRPZUO00 where GZ00IDTEXT = 'Investoren' 

IF @VarGrpId is not null BEGIN 

if object_id('tempdb.dbo.#GesPaths') is not null drop table dbo.#GesPaths

select distinct
a.xgesellschaftercode as FromGesCode
, isnull(fr.xname, a.xgesellschaftercode) as FromName
, a.xgesellschaftcode  as ToGesCode
, isnull(tg.xname, a.xgesellschaftcode) as ToName
, a.Gesellschafter            as FromGesid
, a.Gesellschaft                as ToGesId
, sum(a.DirekterAnteil) as DirekterAnteil
, sum(a.GesamtAnteil) as  GesamtAnteil
--RW: 01.10.21 Zeile hinzugefügt
, sum(isnull(a.xIsSelfAlloc,1)) as Anteil_Factor1
--Ende
, a.Periode
into dbo.#GesPaths
--select * 
from		BISIBOBDB.dbo.G_GesbtlgPath_Period a
join		BISIBOBDB.dbo.xgesellschaft fr on Gesellschafter = fr.xgesellschaftid and fr.ldelete != 1
join		BISIBOBDB.dbo.xgesellschaft tg on Gesellschaft = tg.xgesellschaftid and tg.ldelete != 1
where GesamtAnteil &lt;&gt; 0
group by xgesellschaftercode, fr.xname, a.xgesellschaftcode, tg.xname
, a.Gesellschafter, a.Gesellschaft, a.Periode


--TEXT Löschen der vorhandenen komplett
delete z2
from      dbo.TVARGRPZUO02 z2
join      dbo.TVARGRPZUO01 z1 on z1.GZ01IDNR = z2.PGZ01IDNR
where z1.PGZ00IDNR = @VarGrpID 
 
delete z1
from      dbo.TVARGRPZUO01 z1
where z1.PGZ00IDNR = @VarGrpID 

-- TEXT Hinzufügen der Zeilen

insert into dbo.TVARGRPZUO01
                (PGZ00IDNR, GZ01TEXT, GZ01TNR)
select distinct
9, left(FromName,100), fromGesCode
from                     dbo.#GesPaths a
left join                dbo.TVARGRPZUO01 zuo on a.fromGesCode =  zuo.GZ01TNR and zuo.PGZ00IDNR = 9
where zuo.GZ01TNR is null


update zuo set GZ01TEXT = left(FromName,100)
from		dbo.#GesPaths a
join        dbo.TVARGRPZUO01 zuo on a.fromGesCode =  zuo.GZ01TNR and zuo.PGZ00IDNR = @VarGrpID
where GZ01TEXT &lt;&gt; left(FromName,100)

-- Einfügen der Prozentsätze der Gesellschaften
insert into  dbo.TVARGRPZUO02
--RW 01.10.21: angepasst und Faktor1 hinzugefügt
(PGZ01IDNR,GZ02IDENT1,GZ02IDENT2,GZ02FACTOR, GZ02FACTOR1)
--Ende
select distinct
GZ01IDNR, ToGesCode, Periode,GesamtAnteil/100, Anteil_Factor1
from		dbo.#GesPaths a
join        dbo.TVARGRPZUO01 zuo on a.fromGesCode =  zuo.GZ01TNR and zuo.PGZ00IDNR = @VarGrpID
where TogesCode is not null and GesamtAnteil &lt;&gt; 0.00
and ToGesCode in (select DDIM03ID from BISDBOBDB.dbo.TDIM03)

-- Einfügen fehlende 100 % für Investor an sich + Zukunft 20 Jahre
declare @Stichtag char(6)
set @Stichtag=convert(char(6),right(year(getdate())+20,4)+'12')
insert into  dbo.TVARGRPZUO02
(PGZ01IDNR,GZ02IDENT1,GZ02IDENT2,GZ02FACTOR)
select distinct
GZ01IDNR, GZ01TNR,  DPERIODID, 1
from		dbo.TVARGRPZUO01 z1
left join   dbo.TPERIOD p on  p.dperiodid between '200601' and @Stichtag
left join   dbo.TVARGRPZUO02 z2 on z1.GZ01IDNR = z2.PGZ01IDNR and z1.GZ01TNR = z2.GZ02IDENT1
                                                               and DPERIODID = z2.GZ02IDENT2 
where  z1.PGZ00IDNR = @VarGrpID 
and right(p.dperiodid,2) &lt;&gt; '00'  and len(p.dperiodid) = 6
and z2.PGZ01IDNR is null

select @MaxP = max(Periode) from BISIBOBDB.dbo.G_GesbtlgPath_Period
select @StartP = convert(char(6),DATEADD(month,1,@Maxp + '01'),112)

select @PlanP = MAX(DPERIODID)
from      dbo.TFAKT_XOUT_PROPERTY tf
join      dbo.TPERIOD tp on tf.FPERIOD = tp.FPERIOD

insert into  dbo.TVARGRPZUO02
(PGZ01IDNR,GZ02IDENT1,GZ02IDENT2,GZ02FACTOR)

select distinct qry.PGZ01IDNR,qry.GZ02IDENT1,DPERIODID ,qry.GZ02FACTOR
from (
select zt2.*, tp.DPERIODID
from          dbo.TVARGRPZUO02 zt2
join          dbo.TVARGRPZUO01 z1 on zt2.PGZ01IDNR = GZ01IDNR
left join     dbo.TPERIOD tp on tp.DPERIODID between @StartP and @PlanP
where z1.PGZ00IDNR = @VarGrpID 
and LEN(DPERIODID) = 6 and RIGHT(DPERIODID,2) &lt;&gt; '00'
and zt2.GZ02IDENT2  = @MaxP
) qry
left join dbo.TVARGRPZUO02 x on qry.PGZ01IDNR = x.PGZ01IDNR and qry.GZ02IDENT1 = x.GZ02IDENT1
                               and DPERIODID = x.GZ02IDENT2 
where x.PGZ01IDNR is null

-- Anleger hinzufügen 
IF object_id('BISIBOBDB.dbo.G_GesbtlgAnleger_Period') is not NULL BEGIN 

if object_id('tempdb.dbo.#AnlPaths') is not null drop table dbo.#AnlPaths 

select  xpernr, xpername,  bet.Periode, bet.xgesellschaftcode, Portfolio = t3.DDIM03GRP10,  Anteile = CONVERT(decimal(20,10),SUM(Anteile) / Gesamt) 
into	dbo.#ANLPaths  
from	BISIBOBDB.dbo.G_GesbtlgAnleger_Period bet 
join	dbo.TDIM03GRP t3 on bet.xgesellschaftcode = t3.DDIM03ID 
where bet.Gesamt &lt;&gt; 0 
group by xpernr, xpername,  bet.Periode, bet.xgesellschaftcode, t3.DDIM03GRP10,Gesamt 

insert into dbo.TVARGRPZUO01 (PGZ00IDNR, GZ01TEXT, GZ01TNR)
select distinct
@VarGrpID, left(xpername,100), xpernr 
from	dbo.#ANLPaths 
where xpernr not in(select GZ01TNR from dbo.TVARGRPZUO01 where PGZ00IDNR = @VarGrpID) 

; With GesPath as ( 
select FromGesCode, ToGesCode, Gesamtanteil, Periode from dbo.#GesPaths 
union 
-- falls keine 100% Sätze vorhanden: hinzufügen 
select distinct FromgesCode = xgesellschaftcode, ToGesCode = xgesellschaftcode, Gesamtanteil = 100.00 , anl.Periode  
from		dbo.#ANLPaths anl 
left join	dbo.#GesPaths ges on ges.FromgesCode = anl.xgesellschaftcode and ges.ToGesCode = anl.xgesellschaftcode and ges.Periode = anl.Periode 
where ges.Periode is null 
) 
insert into  dbo.TVARGRPZUO02
(PGZ01IDNR,GZ02IDENT1,GZ02IDENT2,GZ02IDENT3,GZ02FACTOR)
select distinct
GZ01IDNR, ges.ToGesCode, anl.Periode, Portfolio=NULL, Anteile = CONVERT(decimal(20,10),anl.Anteile * GesamtAnteil)  --GesamtAnteil entfernt, 1.00 zum Test 
from	dbo.#ANLPaths anl 
join	GesPath ges on anl.xgesellschaftcode = ges.FromGesCode and anl.Periode = ges.Periode 
join    dbo.TVARGRPZUO01 zuo on anl.xpernr =  zuo.GZ01TNR and zuo.PGZ00IDNR = @VarGrpID
where ges.GesamtAnteil &lt;&gt; 0 


END   -- Anleger vorhanden 

END    -- VARGRP vorhanden 

END    -- Gesellschaften vorhanden</DTS:VariableValue>
            </DTS:Variable>
