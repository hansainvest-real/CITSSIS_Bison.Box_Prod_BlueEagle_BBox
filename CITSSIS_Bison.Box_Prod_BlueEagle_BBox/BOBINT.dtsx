    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{99F1BC0A-C713-46F4-A29E-316EA0960F38}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;if object_id('tempdb.dbo.#clearTfakt') is not null drop table tempdb.dbo.#clearTfakt&#xA;&#xA;select distinct pe.fperiod, pa.fpart, ve.fversion, d1.fdim01&#xA;into tempdb.dbo.#clearTfakt&#xA;-- select *&#xA;from xFakt f with (nolock)&#xA;join tperiod pe on pe.dperiodid = f.fperiod&#xA;join tdim01 d1 on d1.ddim01id = f.fdim01&#xA;join tpart pa on pa.dpartid = f.fpart&#xA;join tversion ve on ve.dversionid = f.fversion&#xA;&#xA;delete tf&#xA;from tfakt tf&#xA;join tempdb.dbo.#clearTfakt x on tf.fperiod = x.fperiod and tf.fpart = x.fpart and tf.fversion = x.fversion and tf.fdim01 = x.fdim01&#xA;&#xA;if object_id('tempdb.dbo.#clearTfakt') is not null drop table tempdb.dbo.#clearTfakt&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="varSQL_cleanTFakt">
      <DTS:VariableValue
        DTS:DataType="8">if object_id('tempdb.dbo.#clearTfakt') is not null drop table tempdb.dbo.#clearTfakt

select distinct pe.fperiod, pa.fpart, ve.fversion, d1.fdim01
into tempdb.dbo.#clearTfakt
-- select *
from xFakt f with (nolock)
join tperiod pe on pe.dperiodid = f.fperiod
join tdim01 d1 on d1.ddim01id = f.fdim01
join tpart pa on pa.dpartid = f.fpart
join tversion ve on ve.dversionid = f.fversion

delete tf
from tfakt tf
join tempdb.dbo.#clearTfakt x on tf.fperiod = x.fperiod and tf.fpart = x.fpart and tf.fversion = x.fversion and tf.fdim01 = x.fdim01

if object_id('tempdb.dbo.#clearTfakt') is not null drop table tempdb.dbo.#clearTfakt
</DTS:VariableValue>
    </DTS:Variable>
