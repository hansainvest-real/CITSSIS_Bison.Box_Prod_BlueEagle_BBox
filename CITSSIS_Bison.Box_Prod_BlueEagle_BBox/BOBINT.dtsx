            DTS:FilterKind="0" />
          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">"
declare @IntMonToGo int = convert(int,'" + @[User::cfgIntMonToGo] + "')
declare @xDate datetime, @xcurDate datetime 
declare @xMM int 

set @xcurDate=dateadd(month,@IntMonToGo,getdate())
set @xMM = 120 + @IntMonToGo

set @xDate=dateadd(mm,-@xMM,convert(datetime,convert(char(6),@xcurDate,112)+'01',112)) -- Startdatum = erster des Monats ohne Uhrzeit 
if object_id('tempdb.dbo.#Hilfsdatum') is null create table dbo.#Hilfsdatum (xStartDate datetime not null, xEndeDate datetime,FPERIOD varchar(6) null) 
truncate table dbo.#Hilfsdatum 
while (@xDate&lt;=@xcurDate) begin insert into dbo.#Hilfsdatum values(@xDate,dateadd(mm,1,@xDate)-1,convert(char(6),@xDate,112)) set @xDate=dateadd(mm,1,@xDate) end 

-- Letzte Periode auf aktuellen Tag setzen
update dbo.#Hilfsdatum set xEndeDate=convert(datetime,convert(char(8),@xcurDate,112),112) where xEndeDate&gt;@xcurDate

INSERT INTO " + @[User::parTGTDB] + ".dbo.XFAKT(FVERSION,FPERIOD,FPART,FDIM01,FDIM02,FDIM03,FDIM04,FDIM05,FDIM06,FDIM07,FVALUE,FUSER,FTIME)
select 
FVERSION	 = 'IST' 
,FPERIOD	= hd.FPERIOD
,FPART		= 'STAT' 
,FDIM01		= 'ST450' 
,FDIM02		= obj.xobjnr
,FDIM03		= isnull(ges.xgesellschaftcode,'NIX') 
,FDIM04		= 'NIX' ,FDIM05 = 'NIX' ,FDIM06 = 'NIX',FDIM07='NIX'
,FVALUE=1.00,FUSER='INTERFACE',FTIME=GETDATE()
from		dbo.xlbiobjekte obj 
left join	dbo.xlbikeyval kv9 on obj.xobjkey9 = kv9.lid 
left join	dbo.xgesellschaft ges on obj.xobjgesellsch = ges.xgesellschaftid  
cross Apply dbo.#Hilfsdatum hd 
where obj.ldelete = 0 
and ((obj.XOBJWIUEBERGDAT is null and isnull(kv9.xkeyalphanr,'') = '9') or obj.XOBJWIUEBERGDAT &lt;= hd.xEndeDate) 
and (obj.xobjverkdat is null or obj.xobjverkdat &gt; hd.xStartDate) 
"</DTS:PropertyExpression>
