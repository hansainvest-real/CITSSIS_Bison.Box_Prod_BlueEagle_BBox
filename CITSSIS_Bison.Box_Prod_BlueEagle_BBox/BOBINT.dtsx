              <DTS:VariableValue
                DTS:DataType="8">select fdim01, fdim02, fdim03=ge.xgesellschaftcode, FDIM04='NIX', FDIM05='NIX', FDIM06='NIX', FDIM07='NIX', fvalue, fperiod=per.dperiodid, fversion='IST', fpart='STATM', fcurrency=isnull(kv.xkeyalphanr,'EUR'), ftime=getdate(), fuser='Interface'
from
(
 select t1.fdim01, t1.fdim02, t1.fvalue, t1.fperiod as fperiod_beg
 , isnull(t2NxtP.FPERIOD, convert(char(8), getdate(), 112)) as fperiod_end
 , RecNxtPer=ROW_NUMBER() over (partition by t1.fdim02, t1.fdim01, t1.FPERIOD order by t2NxtP.fperiod) 
 from BISDBOBDB..TEMP_STAT_TYP1 t1
 left join BISDBOBDB..TEMP_STAT_TYP1 t2NxtP on t1.fdim01 = t2NxtP.fdim01 and t1.fdim02 = t2NxtP.fdim02 and t2NxtP.fperiod &gt; t1.fperiod
) x
join xlbiobjekte obj on x.fdim02 = obj.xobjnr
join xgesellschaft ge on obj.xobjgesellsch = ge.xgesellschaftid
left join xlbikeyval kv on obj.xobjwaehrung = kv.lid
join BISDBOBDB..TPERIOD per on per.DPERIODID &gt;= x.fperiod_beg and per.DPERIODID &lt; isnull(x.fperiod_end, isnull(convert(char(06), xobjwiuebergdatvk, 112), '999912'))
where RecNxtPer = 1
and fvalue is not null
-- bereits verkaufte Objekte nicht mehr berücksichtigen
and isnull(convert(char(06), xobjwiuebergdatvk, 112), '999912') &gt; convert(char(06), getdate(), 112)

union all

select fdim01, fdim02, fdim03=ge.xgesellschaftcode, FDIM04='NIX', FDIM05='NIX', FDIM06='NIX', FDIM07='NIX', fvalue, fperiod=per.dperiodid, fversion='ISTW', fpart='STATM', fcurrency=isnull(kv.xkeyalphanr,'EUR'), ftime=getdate(), fuser='Interface'
from
(
 select t1.fdim01, t1.fdim02, t1.fvalue, t1.fperiod as fperiod_beg
 , isnull(t2NxtP.FPERIOD, convert(char(8), getdate(), 112)) as fperiod_end
 , RecNxtPer=ROW_NUMBER() over (partition by t1.fdim02, t1.fdim01, t1.FPERIOD order by t2NxtP.fperiod) 
 from BISDBOBDB..TEMP_STAT_TYP2 t1
 left join BISDBOBDB..TEMP_STAT_TYP2 t2NxtP on t1.fdim01 = t2NxtP.fdim01 and t1.fdim02 = t2NxtP.fdim02 and t2NxtP.fperiod &gt; t1.fperiod
 where t1.rec = 1 -- REC=1 (aktuellster Satz je Periode, falls in einem Monat mehrere Gutachen vorhanden)
) x
join xlbiobjekte obj on x.fdim02 = obj.xobjnr
join xgesellschaft ge on obj.xobjgesellsch = ge.xgesellschaftid
left join xlbikeyval kv on obj.xobjwaehrung = kv.lid
join BISDBOBDB..TPERIOD per on per.DPERIODID &gt;= x.fperiod_beg and per.DPERIODID &lt; isnull(x.fperiod_end, isnull(convert(char(06), xobjwiuebergdatvk, 112), '999912'))
where RecNxtPer = 1
-- bereits verkaufte Objekte nicht mehr berücksichtigen
and isnull(convert(char(06), xobjwiuebergdatvk, 112), '999912') &gt; convert(char(06), getdate(), 112)</DTS:VariableValue>
