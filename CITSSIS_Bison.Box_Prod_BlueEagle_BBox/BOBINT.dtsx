                <SQLTask:SqlTaskData
                  SQLTask:Connection="{776C6DFC-903B-457E-8010-EDEF3CA338EB}"
                  SQLTask:SqlStatementSource="-- temporäre Tabellen bereinigen&#xA;if object_id('tempdb.dbo.#EDarlKondi') is not null drop table dbo.#EDarlKondi&#xA;if object_id('tempdb.dbo.#EdBetrag') is not null drop table dbo.#EdBetrag&#xA;if object_id('tempdb.dbo.#XFAKT') is not null drop table dbo.#XFAKT&#xA;&#xA;declare @xDate     as datetime&#xA;declare @xcurDate  as datetime&#xA;declare @peKurs as varchar(10) ='*LAST'&#xA;-- Hilfstabelle mit Gültigkeiten der Konditionen erstellen&#xA;select&#xA;ed.xednr&#xA;, lid = ed.lid&#xA;, KondLid = kd.lid&#xA;, kd.xkondbeginn&#xA;, KondBeginn = convert(char(6),kd.xkondbeginn,112)&#xA;, xKondEnde =                 min(isnull((dateadd(month,-1,kd2.xkondbeginn)),'9999-12-31T00:00:00'))&#xA;, KondEnde  = convert(char(6),min(isnull((dateadd(month,-1,kd2.xkondbeginn)),'9999-12-31T00:00:00')),112)&#xA;, xEnde     = isnull(kd.xkondendfaellig,kd.xkondTilgEnde)&#xA;, kd.xKondZinsBindEnde&#xA;into dbo.#EDarlKondi&#xA;from&#x9;&#x9;dbo.xlbieinzeldarl ed&#xA;join&#x9;&#x9;dbo.xlbidarlkondi kd on ed.lid = kd.lparentid&#xA;left join&#x9;dbo.xlbidarlkondi kd2 on ed.lid = kd2.lparentid  and kd.lid &lt;&gt; kd2.lid and kd.xkondbeginn &lt; kd2.xkondbeginn and kd2.ldelete = 0&#xA;where ed.ldelete = 0 and kd.ldelete = 0&#xA;group by ed.xednr, ed.lid, kd.lid,  kd.xkondbeginn ,isnull(kd.xkondendfaellig,kd.xkondTilgEnde) , kd.xKondZinsBindEnde&#xA;&#xA;-- Ende begrenzen auf aktuelles Datum&#xA;update&#xA;dbo.#EDarlKondi&#xA;set KondEnde = convert(char(6),isnull(xende, getdate()),112)&#xA;where Kondende = '999912'&#xA;&#xA;-- Hilfstabelle mit Beträgen und Gültigkeit erstellen&#xA;&#xA;select b1.*, convert(char(6),b1.xedPeriod,112) as StartPeriode, convert(varchar(6), NULL) as EndePeriode&#xA;, xwaehrgzu = ISNULL(obj.xobjwaehrung,dft.lid) &#xA;, CURRENCY  = ISNULL(kv.xkeyalphanr,dft.xkeyalphanr) &#xA;into dbo.#EdBetrag&#xA;from dbo.xlbiEinzeldBetrag b1&#xA;join dbo.xlbieinzeldarl ed on b1.lparentid = ed.lid &#xA;left join dbo.xlbiobjekte obj on ed.xobjnr = obj.lid &#xA;left join dbo.xlbikeyval kv on obj.xobjwaehrung = kv.lid &#xA;cross apply (select lid = keyval_lid, xkeyalphanr  from dbo.V_xkeyhead_xkeyval where xkeyhmc = 'Waehrungen' and xkeyalphanr = 'EUR') dft &#xA;where b1.ldelete = 0 and ed.ldelete = 0 &#xA;and b1.xedPeriod in (select max(xedperiod) from dbo.xlbiEinzeldBetrag b2&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;where b2.ldelete = 0 and b2.lparentid = b1.lparentid&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;and   convert(char(6),b2.xedPeriod,112) = convert(char(6),b1.xedPeriod,112))&#xA; &#xA;&#xA;-- Endtermine festlegen&#xA;&#xA;update ed set EndePeriode = x.EndePeriode&#xA;from&#x9;dbo.#EdBetrag ed&#xA;&#xA;join (select a.lparentid, a.Startperiode, isnull(min(b.STartPeriode),'999912') as EndePeriode&#xA;&#x9;&#x9;from&#x9;&#x9;dbo.#EdBetrag a&#xA;&#x9;&#x9;left join&#x9;dbo.#EdBetrag b on a.lparentid = b.lparentid and a.StartPeriode &lt; b.StartPeriode&#xA;&#x9;&#x9;group by a.lparentid, a.Startperiode) x on ed.lparentid = x.lparentid and ed.Startperiode = x.Startperiode&#xA;&#xA;&#xA;&#xA;  select @xDate=convert(datetime,min(Startperiode)+ '01',112) from dbo.#EdBetrag  -- Startdatum = erster des Monat des Zeitraums &#xA;   --select @xcurDate=dateadd(day,-1,dateadd(month,1,CONVERT(datetime,max(KondEnde) +'01',112))) from  dbo.#EDarlKondi   &#xA;   select @xcurDate = GETDATE() &#xA;   if object_id('tempdb.dbo.#Hilfsdatum') is null create table dbo.#Hilfsdatum (xStartDate datetime not null, xEndeDate datetime) &#xA;   truncate table dbo.#Hilfsdatum &#xA;   while (@xDate&lt;=@xcurDate) begin insert into dbo.#Hilfsdatum values(@xDate,dateadd(mm,1,@xDate)-1) set @xDate=dateadd(mm,1,@xDate) end &#xA;   -- Letzte Periode auf aktuellen Tag setzen&#xA;   update dbo.#Hilfsdatum set xEndeDate=convert(datetime,convert(char(8),@xcurDate,112),112) where xEndeDate&gt;@xcurDate&#xA;&#xA;   if object_id('tempdb.dbo.#waehrgkurse_1') is not null drop table dbo.#waehrgkurse_1&#xA;   select cast(substring(max(convert(varchar(20),xwaehrgbeginn,112)+cast(ku.lid as varchar(20))),9,50) as int) as ku_lid,&#xA;          convert(char(6),xEndeDate,112) collate Latin1_General_CI_AS as xkuPeriod&#xA;   into       dbo.#waehrgkurse_1&#xA;   from       dbo.xlbiwaehrgkurse   ku&#xA;   inner join dbo.#Hilfsdatum       dt &#xA;         on isnull(xwaehrgbeginn,-53690)&lt;=case when @peKurs='*LAST' then xEndeDate else xStartDate end &#xA;   where ku.ldelete=0 &#xA;&#x9;and ku.xwaehrgzu in(select distinct xwaehrgzu from dbo.#EdBetrag ed)  &#xA;   group by xwaehrgvon,xwaehrgzu,convert(char(6),xEndeDate,112) collate Latin1_General_CI_AS&#xA;   -- 2. Hilfstabelle für Waehrungskurse aufbauen  &#xA;   if object_id('tempdb.dbo.#waehrgkurse_2') is not null drop table dbo.#waehrgkurse_2&#xA;   select * into dbo.#waehrgkurse_2 from dbo.#waehrgkurse_1 inner join dbo.xlbiwaehrgkurse on lid=ku_lid&#xA;&#xA;&#xA;select *&#xA;into dbo.#XFAKT&#xA;from (select FPART     = 'STATM'&#xA;           , FVERSION  = 'IST'&#xA;           , FDIM01    = 'ST510'&#xA;           , FDIM02    = obj.xobjnr&#xA;           , FDIM03    = isnull(ges.xgesellschaftcode,'NIX')&#xA;           , FDIM04    = 'NIX'&#xA;           , FDIM05    = 'NIX'&#xA;           , FDIM06    = 'NIX'&#xA;           , FDIM07    = 'NIX'&#xA;           , FPERIOD   = DPERIODID&#xA;           , FVALUE    = datediff(mm,xStichtag ,ISNULL(xKondZinsBindEnde,'2099-12-31T00:00:00'))* BT.xedBetrag * xwaehrgkurs&#xA;           , FCURRENCY = bt.CURRENCY &#xA;      from(select DPERIODID, xStichtag = convert(datetime,DATEADD(month,1,DPERIODID + '01') -1)  from BISDBOBDB.dbo.TPERIOD b where CONVERT(char(6),getdate(),112) &gt;= DPERIODID) tp&#xA;      join      dbo.#EdarlKondi     ed on ed.xkondbeginn &lt;= tp.xStichtag&#xA;                                      and ed.xKondEnde   &gt;  tp.xStichtag&#xA;      join      dbo.#EdBetrag       bt on ed.lid          = bt.lparentid&#xA;                                      and DPERIODID      &gt;= bt.Startperiode&#xA;                                      and DPERIODID      &lt;  bt.EndePeriode&#xA;      join      dbo.xlbieinzeldarl eda on ed.lid          = eda.lid&#xA;&#x9;  left join   dbo.#waehrgkurse_2 whrg on DPERIODID = whrg.xkuPeriod and eda.xedKreditWaehrung = whrg.xwaehrgvon and bt.xwaehrgzu = whrg.xwaehrgzu &#xA;      left join dbo.xlbiobjekte    obj on eda.xobjnr  = obj.lid&#xA;      left join dbo.xgesellschaft  ges on obj.xobjgesellsch = ges.xgesellschaftid&#xA;      where obj.xobjnr is not null&#xA;        and xKondZinsBindEnde &gt; xStichtag) qry&#xA;where FVALUE &lt;&gt; 0&#xA;&#xA;insert into BISDBOBDB.dbo.XFAKT&#xA;       (FVERSION, FPERIOD, FPART, FDIM01, FDIM02, FDIM03, FDIM04, FDIM05, FDIM06, FDIM07,    FVALUE , FCURRENCY,  FUSER      , FTIME    )&#xA;select  FVERSION, FPERIOD, FPART, FDIM01, FDIM02, FDIM03, FDIM04, FDIM05, FDIM06, FDIM07,SUM(FVALUE), FCURRENCY,'INTERFACE', GETDATE()&#xA;from dbo.#XFAKT&#xA;GROUP BY FVERSION, FPERIOD, FPART, FDIM01, FDIM02, FDIM03, FDIM04, FDIM05, FDIM06, FDIM07, FCURRENCY&#xA;&#xA;--cleanup&#xA;if object_id('tempdb.dbo.#EDarlKondi')is not null drop table dbo.#EDarlKondi&#xA;if object_id('tempdb.dbo.#EdBetrag'  )is not null drop table dbo.#EdBetrag&#xA;if object_id('tempdb.dbo.#XFAKT'     )is not null drop table dbo.#XFAKT" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
