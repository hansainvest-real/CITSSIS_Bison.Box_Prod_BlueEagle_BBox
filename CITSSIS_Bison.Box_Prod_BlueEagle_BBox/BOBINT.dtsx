-- Anteilspreis kaufm. runden
-- RS 2021-03-04: Anpassung wegen Fehler bei Datenkonvertierung durch attrToRound.val
--case when isnull(attrToRound.val, 0) = 1 
case when isnull(attrToRound.val, 'nicht runden') = 'runden'
then ROUND(SUM(sumCompany.fvalue) over (partition by sumCompany.fdim03 order by sumCompany.fperiod rows unbounded preceding) / fundsShares.FVALUE, 2)
else ROUND(SUM(sumCompany.fvalue) over (partition by sumCompany.fdim03 order by sumCompany.fperiod rows unbounded preceding) / fundsShares.FVALUE, 2, 1)
end as FVALUE

, FDIM01='ST750', fundsShares.FDIM02, sumCompany.FDIM03, fundsShares.FDIM04, fundsShares.FDIM05, fundsShares.FDIM06, fundsShares.FDIM07, fundsShares.FPART, fundsShares.FPERIOD, fundsShares.FVERSION
, FUSER='interface', FTIME=GETDATE()
from 
(
-- je Gesellschaft die Vermögenswerte aufsummieren
select d3grp.DDIM03GRP02, facts.FDIM03, FPERIOD, sum(FVALUE) as FVALUE
from VFAKT_KUM facts
join TDIM03GRP d3grp on facts.fdim03 = d3grp.ddim03id
where FVERSION = 'IST'
and FPART = 'FIBU_kum'
and FDIM01 in 
(
		select ddim01id
		from TDIM01GRP 
		where DDIM01GRP47 = '5'
)
group by d3grp.DDIM03GRP02, facts.FDIM03, FPERIOD
) sumCompany
-- Umlaufenden Anteile
join (select FDIM02, FDIM03, FDIM04, FDIM05, FDIM06, FDIM07, FPART, FPERIOD, FVERSION, case when FVALUE = 0 then 1 else FVALUE end as FVALUE
		from VFAKT 
		where FDIM01 = 'ST700' and FVERSION = 'IST'
) fundsShares on sumCompany.DDIM03GRP02 = fundsShares.FDIM03 and sumCompany.FPERIOD = fundsShares.FPERIOD
left join
(
-- Steuerung der Rundung
select xfondsnr, case when attrVal.xAttrBit = 1 then 'runden' else 'nicht runden' end as val
from BISIBOBDB.dbo.xlbiAttribute attr
join BISIBOBDB.dbo.xlbiAttributeInh attrVal on attr.lid = attrval.xAttrID
join BISIBOBDB.dbo.xlbifonds fo on attrVal.lparentid = fo.lid
where xAttrAlphanr = 'Z_FONDS_SONSTIGE_040' -- Share price commercial rounding
--and attrVal.ldelete = 0
) attrToRound on xfondsnr = fundsShares.FDIM03</property>
