                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--insert into BISDBOBDB.dbo.XFAKT
select 
  FVERSION='IST'
, FPERIOD
, FPART='STATP'
, FDIM01='ST700'
, FDIM02 = x.gesCode + ' 09999'
, FDIM03 = x.gesCode
, FDIM04='NIX',FDIM05='NIX',FDIM06='NIX',FDIM07='NIX'
, FVALUE = SUM(zccshares)
, FMULTI = NULL
, FUSER='INTERFACE'
, FTIME=GETDATE()
, FTEXT = NULL
, FUNIT = NULL
, FCURRENCY = 'NIX'
from
(
	Select ges.xgesellschaftcode as gesCode, inv.xpernr, zccshares,   convert(char(6),isnull(zccvaluta,zccduedate),112) as FPERIOD 
	from zInvestor a
	inner join xgesellschaft ges on a.lparentid = ges.lid
	inner join xlbiperadr inv on inv.lid = a.zinvInvestorID
	inner join zCapitalCall ant on ant.lparentid = a.lid
	inner join xlbikeyval cctyp on cctyp.lid = ant.zccType
	inner join xlbifonds fo on ges.xGesellFonds = fo.lid
	inner join xlbiperadr foClient on fo.xfondsClient = foclient.lid
	where 1=1
	and fo.xfondsnr not like 'Z%'
	and cctyp.xkeyalphanr not in ('TAT_10', 'TAT_15') 
	and ant.ldelete = 0
	and a.ldelete = 0
) x
group by x.gesCode, FPERIOD</property>
