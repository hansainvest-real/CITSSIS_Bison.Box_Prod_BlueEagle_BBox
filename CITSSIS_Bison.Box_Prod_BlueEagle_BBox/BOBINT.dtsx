          <DTS:Variables />
          <DTS:LoggingOptions
            DTS:FilterKind="0" />
          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">REPLACE(
"DECLARE @cr VARCHAR(2), @cmd varchar(max);
SET @cr = convert(VARCHAR(2),0x0a);
WITH _ObjAttribute AS(SELECT xAttrGrpId=g.lid
                           , xAttrId   =a.lid
                           , xAttrKeyDspExpression
                           , xAttrTyp
                           , xAttrITyp      = case when a.xattrchar=1 then 1 when a.xAttrBit=1 then 2 when a.xAttrVar=1 then 3 when a.xAttrDat=1 then 4 when a.xAttrKeyHead is not null then 5 end
                           , xAttrNr        = ATTRNR
                           , xAttrDim       = ATTRDIM
                           , xAttrDimGrp    = ATTRGRP
