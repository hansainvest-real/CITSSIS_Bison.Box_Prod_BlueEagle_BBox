                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--BestCase =&gt; definitves Vertragsende
Select 'ST610' as FDIM01
     , T02G.DDIM02GRP30 as FDIM02
     , VM  .FDIM03
     , 'NIX' as FDIM04
     , 'NIX' as FDIM05
     , 'NIX' as FDIM06
     , 'NIX' as FDIM07
     , VM.DPERIODID as FPERIOD
     , 'STATM' as FPART
     , 'IST' as FVERSION
     , SUM(datediff(mm,xStichtag ,isnull(T02.ATTR0DATE03,dateadd(mm,12,xStichtag)))* VM.FVALUE) as FVALUE
     , 0 as FMULTI
     , 'Interface' as FUSER
     , NULL as FTEXT
     , NULL as FUNIT
     , NULL as FCURRENCY
     , GETDATE() as FTIME
FROM(Select TP.DPERIODID, fdim02, FDIM03, TP.xStichtag, SUM (FVALUE) as Fvalue from VFAKT V
     cross join (select DPERIODID, xStichtag = convert(datetime,DATEADD(month,1,DPERIODID + '01') -1) from dbo.TPERIOD b where CONVERT(char(6),getdate(),112) &gt;= DPERIODID) TP
     where V.FDIM01   = 'ST210'
       and V.FPART    = 'STAT'
       and V.FVERSION = 'IST'
       and V.FPERIOD &lt;= TP.DPERIODID
     group by FDIM02, FDIM03, TP.DPERIODID, TP.xStichtag) VM
inner join(select fdim02, ddim02id, ATTR0DATE03 = case WHEN ATTR0DATE03 != '9999-12-31T00:00:00' THEN ATTR0DATE03 END FROM  TDIM02) T02 on VM.FDIM02 = T02.DDIM02ID
inner join TDIM02GRP T02G on T02G.FDIM02 = T02.FDIM02
group by T02G.DDIM02GRP30, VM.FDIM03, VM.DPERIODID
