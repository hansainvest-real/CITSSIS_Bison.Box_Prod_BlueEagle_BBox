          DTS:ThreadHint="4">
          <DTS:Variables />
          <DTS:LoggingOptions
            DTS:FilterKind="0" />
          <DTS:PropertyExpression
            DTS:Name="SqlStatementSource">"declare @StartP as varchar(6)
declare @PlanP as varchar(6)
declare @MaxP as varchar(6)
declare @MinP as varchar(6) 
declare @VarGrpID int 

IF object_id('"+@[User::parSRCDB]+".dbo.G_GesbtlgAnleger_Period') is not null BEGIN 

select @VarGrpID = GZ00IDNR from dbo.TVARGRPZUO00 where GZ00IDTEXT = 'Anleger' 

IF @VarGrpId is not null BEGIN 

--TEXT Löschen der vorhandenen komplett
delete z2
from      dbo.TVARGRPZUO02 z2
join        dbo.TVARGRPZUO01 z1 on z1.GZ01IDNR = z2.PGZ01IDNR
where z1.PGZ00IDNR = @VarGrpID 
 
delete z1
from      dbo.TVARGRPZUO01 z1
where z1.PGZ00IDNR = @VarGrpID 

-- TEXT Hinzufügen der Zeilen

insert into dbo.TVARGRPZUO01
                (PGZ00IDNR, GZ01TEXT, GZ01TNR)
select distinct
@VarGrpID, left(xpername,100), xpernr 
from                     "+@[User::parSRCDB]+".dbo.G_GesbtlgAnleger_Period a 
left join                dbo.TVARGRPZUO01 zuo on a.xpernr =  zuo.GZ01TNR and zuo.PGZ00IDNR = @VarGrpID
where zuo.GZ01TNR is null

update zuo set GZ01TEXT = left(xpername,100)
from		"+@[User::parSRCDB]+".dbo.G_GesbtlgAnleger_Period a 
join        dbo.TVARGRPZUO01 zuo on a.xpernr =  zuo.GZ01TNR and zuo.PGZ00IDNR = @VarGrpID
where GZ01TEXT &lt;&gt; left(xpername,100)

-- Einfügen der Prozentsätze der Fonds 
insert into  dbo.TVARGRPZUO02
(PGZ01IDNR,GZ02IDENT1,GZ02IDENT2,GZ02FACTOR)
select distinct
GZ01IDNR, DDIM03GRP10, Periode,Anteile 
from	(select xpernr, xpername, Gesamt, Periode, t3.DDIM03GRP10, Anteile = SUM(Anteile) / Gesamt 
		from	"+@[User::parSRCDB]+".dbo.G_GesbtlgAnleger_Period bet 
		join	TDIM03GRP t3 on bet.xgesellschaftcode = t3.DDIM03ID 
		group by   xpernr, xpername, Gesamt, Periode, t3.DDIM03GRP10) a  
join    dbo.TVARGRPZUO01 zuo on a.xpernr =  zuo.GZ01TNR and zuo.PGZ00IDNR = @VarGrpID
where DDIM03GRP10 is not null 
and DDIM03GRP10 in (select DDIMGRPID from dbo.TDIMGRPTXT where DDIMNR = 2 and DDIMGRPNR = 10)
END 
END 
"</DTS:PropertyExpression>
